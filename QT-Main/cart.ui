     <property name="styleSheet">
      <string notr="true">/* tableCart는 “투명 컨테이너”처럼 */
#tableCart {
    background: transparent;
    border: none;
    gridline-color: transparent;
    outline: 0;
}

#tableCart::item:selected,
#tableCart::item:focus {
    background: transparent;
    outline: 0;
}

/* viewport 투명 */
#tableCart QAbstractScrollArea::viewport {
    background: transparent;
}

/* 세로 스크롤바 깔끔하게 */
#tableCart QScrollBar:vertical {
    background: transparent;
    width: 10px;
    margin: 6px 2px 6px 2px;
}
#tableCart QScrollBar::handle:vertical {
    background: #e5e7eb;
    border-radius: 5px;
    min-height: 28px;
}
#tableCart QScrollBar::handle:vertical:hover {
    background: #d1d5db;
}
#tableCart QScrollBar::add-line:vertical,
#tableCart QScrollBar::sub-line:vertical {
    height: 0px;
}
#tableCart QScrollBar::add-page:vertical,
#tableCart QScrollBar::sub-page:vertical {
    background: transparent;
}
